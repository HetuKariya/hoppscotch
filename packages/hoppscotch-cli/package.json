{
  "name": "@hoppscotch/cli",
  "version": "0.11.1",
  "description": "A CLI to run Hoppscotch test scripts in CI environments.",
  "homepage": "https://hoppscotch.io",
  "type": "module",
  "main": "dist/index.js",
  "bin_hopp": "bin/hopp.js",
  "publishConfig_access": "public",
  "engines_node": ">=18",
  "scripts_build": "pnpm exec tsup",
  "scripts_dev": "pnpm exec tsup --watch",
  "scripts_debugger": "node debugger.js 9999",
  "scripts_prepublish": "pnpm exec tsup",
  "scripts_prettier-format": "prettier --config .prettierrc 'src//*.ts' --write",
  "scripts_test": "pnpm run build && vitest run",
  "scripts_do-typecheck": "pnpm exec tsc --noEmit",
  "scripts_do-test": "pnpm run test",
  "keywords_0": "cli",
  "keywords_1": "hoppscotch",
  "keywords_2": "hopp-cli",
  "author": "Hoppscotch (support@hoppscotch.io)",
