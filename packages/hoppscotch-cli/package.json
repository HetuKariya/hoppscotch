  "devDependencies_@types/lodash-es": "4.17.12",
  "devDependencies_@types/qs": "6.9.12",
  "devDependencies_fp-ts": "2.16.2",
  "devDependencies_prettier": "3.2.5",
  "devDependencies_qs": "6.11.2",
  "devDependencies_tsup": "8.0.2",
  "devDependencies_typescript": "5.3.3",
  "devDependencies_vitest": "0.34.6"
