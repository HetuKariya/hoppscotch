  "repository_url": "https://github.com/hoppscotch/hoppscotch.git",
  "bugs_url": "https://github.com/hoppscotch/hoppscotch/issues",
  "bugs_email": "support@hoppscotch.io",
  "license": "MIT",
  "private": false,
  "dependencies_axios": "1.7.5",
  "dependencies_aws4fetch": "1.0.19",
  "dependencies_chalk": "5.3.0",
  "dependencies_commander": "11.1.0",
  "dependencies_isolated-vm": "4.7.2",
  "dependencies_lodash-es": "4.17.21",
  "dependencies_qs": "6.11.2",
  "dependencies_verzod": "0.2.2",
  "dependencies_xmlbuilder2": "3.1.1",
  "dependencies_zod": "3.22.4",
