{
  "build.beforeDevCommand": "pnpm dev",
  "build.beforeBuildCommand": "pnpm build",
  "build.devPath": "http://localhost:3000",
  "build.distDir": "../dist",
  "build.withGlobalTauri": false,
  "package.productName": "Hoppscotch",
  "package.version": "24.9.1",
  "tauri.allowlist.all": false,
  "tauri.allowlist.shell.all": false,
  "tauri.allowlist.shell.open": true,
  "tauri.allowlist.os.all": true,
  "tauri.allowlist.fs.writeFile": true,
  "tauri.allowlist.dialog.save": true,
  "tauri.allowlist.http.all": true,
  "tauri.allowlist.http.request": true,
  "tauri.allowlist.http.scope": [
    "http://*",
    "https://*",
    "wss://*"
  ],
  "tauri.allowlist.window.startDragging": true,
  "tauri.bundle.active": true,
  "tauri.bundle.category": "DeveloperTool",
  "tauri.bundle.icon": [
    "icons/32x32.png",
    "icons/128x128.png",
    "icons/128x128@2x.png",
    "icons/icon.icns",
    "icons/icon.ico"
  ],
  "tauri.bundle.identifier": "io.hoppscotch.desktop",
  "tauri.bundle.targets": "all",
  "tauri.security.csp": null,
  "tauri.updater.active": false,
  "tauri.windows.0.label": "main",
  "tauri.windows.0.title": "Hoppscotch",
  "tauri.windows.0.visible": false,
  "tauri.windows.0.fullscreen": false,
  "tauri.windows.0.resizable": true,
  "tauri.windows.0.width": 800,
  "tauri.windows.0.height": 600,
  "tauri.windows.0.fileDropEnabled": false
}